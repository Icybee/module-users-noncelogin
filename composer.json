{
	"name": "icybee/module-users-noncelogin",
	"type": "icanboogie-module",
	"description": "Provides nonce login tickets to users who have forgot their password.",
	"keywords": [
		"user",
		"nonce login"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-users-noncelogin/issues",
		"source": "https://github.com/Icybee/module-users-noncelogin"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": "^5.6 || ^7.0",
		"icanboogie/module-installer": "^1.2",
		"icybee/module-users": "^4.0",
		"icybee/module-registry": "^4.0",
		"brickrouge/bind-icanboogie-validate": "dev-master"
	},
	"autoload": {
